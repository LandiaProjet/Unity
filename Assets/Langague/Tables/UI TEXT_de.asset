  - m_Id: 15564151296000
    m_Localized: Besiegen
    m_Metadata:
      m_Items: []
  - m_Id: 15876421423104
    m_Localized: Ergebnis
    m_Metadata:
      m_Items: []
  - m_Id: 16586160574464
    m_Localized: FERTIG!
    m_Metadata:
      m_Items: []
  - m_Id: 17665610846208
    m_Localized: "Best\xE4tigen"
    m_Metadata:
      m_Items: []
  - m_Id: 17948512456704
    m_Localized: Kaufen
    m_Metadata:
      m_Items: []
  - m_Id: 1897957111992320
    m_Localized: "Wir brauchen deine Hilfe, ich wei\xDF, dass du der St\xE4rkste
      in dieser Stadt bist! Deine Aufgabe ist einfach, du musst den heiligen Stein
      finden, der uns gestohlen wurde!"
    m_Metadata:
      m_Items: []
  - m_Id: 1904847715930112
    m_Localized: "Ich denke, du kannst das auch alleine schaffen. Ich w\xFCnsche
      dir viel Gl\xFCck. Ade!"
    m_Metadata:
      m_Items: []
  - m_Id: 1907131367657472
    m_Localized: Pass auf!
    m_Metadata:
      m_Items: []
  - m_Id: 1907510406909952
    m_Localized: "Du hast keine M\xFCnzen mehr! Du kannst dir einen Werbespot ansehen,
      um dich wieder aufzuladen!"
    m_Metadata:
      m_Items: []
  - m_Id: 1908064411553792
    m_Localized: Schau es dir an!
    m_Metadata:
      m_Items: []
  - m_Id: 1908155113377792
    m_Localized: Nein danke
    m_Metadata:
      m_Items: []
  - m_Id: 1908729703665664
    m_Localized: "Du hast kein Herz! Sie k\xF6nnen sich einen Werbespot ansehen,
      um wieder aufzuladen!"
    m_Metadata:
      m_Items: []
  - m_Id: 1909243816284160
    m_Localized: Anzeige ansehen
    m_Metadata:
      m_Items: []
  - m_Id: 1909635732049920
    m_Localized: Laden Sie all Ihre Herzen auf
    m_Metadata:
      m_Items: []
  - m_Id: 1909913566941184
    m_Localized: 1 Monat unendliches Herz
    m_Metadata:
      m_Items: []
  - m_Id: 1910139149193216
    m_Localized: 3 Monate unendliches Herz
    m_Metadata:
      m_Items: []
  - m_Id: 1910298029428736
    m_Localized: 6 Monate unendliches Herz
    m_Metadata:
      m_Items: []
  - m_Id: 1911194901647360
    m_Localized: Gesundheit
    m_Metadata:
      m_Items: []
  - m_Id: 1911530873786368
    m_Localized: Gold
    m_Metadata:
      m_Items: []
  - m_Id: 1913101028265984
