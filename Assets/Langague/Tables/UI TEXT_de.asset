  m_TableData:
  - m_Id: 1145882363138048
    m_Localized: "Hallo! Willkommen in der Stadt Kilawa! Ich bin der Chef dieses
      Dorfes! Ich wei\xDF, ich wei\xDF, es ist zu gut, aber lass uns nicht dar\xFCber
      reden, lass uns direkt zu den ernsten Dingen kommen..."
    m_Metadata:
      m_Items: []
  - m_Id: 1911008808767488
    m_Localized: '1 Jahr vom Herzen

      unendlich'
    m_Metadata:
      m_Items: []
  - m_Id: 7125438464
    m_Localized: Mission
    m_Metadata:
      m_Items: []
  - m_Id: 611851800576
    m_Localized: Einstellungen
